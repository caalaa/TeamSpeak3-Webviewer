<?xml version="1.0" encoding="UTF-8"?>
<config>
    <info>
        <author>Max Rath</author>
        <homepage>http://maxesstuff.de</homepage>
        
        <!-- Don't change anything here -->
        <!-- Ändern Sie hier nichts! -->
        <abstract>false</abstract>
    </info>

    <!-- If this is setted to true, the user can join a username before connecting to the server -->
    <!-- Wenn auf true gesetzt, wird der Benutzer aufgefordet, einen Nicknamen zu wählen -->
    <prompt_username>true</prompt_username>
    
    <!-- If your server has a password and the user should be asked for it, set this to true -->
    <!-- Wenn der Server ein Passwort hat und der Benutzer danach gefragt werden soll, setzen Sie dies auf true -->
    <prompt_pass>false</prompt_pass>
    
    <!-- If the server has a password and it should be automatically added to the login, enter the password here-->
    <!-- Wenn der Server ein Passwort hat und dieses direkt beim Login übergeben werden soll, geben Sie das Passwort hier ein -->
    <server_password>none</server_password>
    
    <!-- If you have entered 127.0.0.1 or localhost as host into the config file, enter the external IP or host here -->
    <!-- Wenn Sie in der Konfiguration 127.0.0.1 oder localhost eingeben habem so tragen Sie hier die externe IP des Servers hier ein -->
    <connect_host>none</connect_host>
</config>
